{
  "name": "telegramprinter",
  "version": "1.0.0",
  "description": "Telegram bot which manages printer",
  "main": "./index.js",
  "scripts": {
    "build": "tsc && mkdir ./build/webapp && cp -r ./webapp/* ./build/webapp/ && cp ./package*.json ./build/ && cd ../../",
    "start": "cp -r ./node_modules/* ./build/node_modules/ && cd ./build && node .",
    "test": "cd ./build/ && node index.spec.js && cd ../"
  },
