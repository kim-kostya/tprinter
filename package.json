  "license": "GPL-3.0-or-later",
  "dependencies": {
    "express": "^4.17.1",
    "node-downloader-helper": "^1.0.18",
    "sqlite3": "^5.0.2",
    "telegraf": "^4.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.6.0",
    "@types/sqlite3": "^3.1.7"
  }
}
